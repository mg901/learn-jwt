{
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "consistent-return": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,

    "newline-before-return": 2
  }
}
